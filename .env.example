# Quality Assurance Configuration
VITE_QA_API_URL=http://localhost:3001

# AWS Configuration for QA Services
AWS_REGION=eu-central-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Optional: Snyk Token for Security Scanning
SNYK_TOKEN=your_snyk_token_here

# Optional: SonarQube Token for Quality Gates
SONAR_TOKEN=your_sonar_token_here

# QA Server Configuration
QA_SERVER_PORT=3001

# Bedrock Support Mode Configuration
# Feature Flags
VITE_ENABLE_BEDROCK_SUPPORT_MODE=false
VITE_ENABLE_INTELLIGENT_ROUTING=true
VITE_ENABLE_DIRECT_BEDROCK_FALLBACK=false

# Support Mode Configuration
VITE_BEDROCK_AUDIT_INTERVAL=600000
VITE_BEDROCK_MONITORING_LEVEL=basic
VITE_BEDROCK_AUTO_RESOLUTION_ENABLED=false

# Environment-specific settings
VITE_BEDROCK_DEBUG_MODE=false
VITE_BEDROCK_VERBOSE_LOGGING=false
VITE_BEDROCK_MOCK_INFRASTRUCTURE_AUDIT=false

# AWS Configuration for Bedrock
VITE_BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0

# Safety Settings
VITE_BEDROCK_RATE_LIMIT_ENABLED=true
VITE_BEDROCK_COST_LIMIT_ENABLED=true
VITE_BEDROCK_MAX_REQUESTS_PER_MINUTE=30

# Notification Settings
VITE_BEDROCK_NOTIFICATION_CHANNELS=console,webhook
VITE_BEDROCK_SLACK_WEBHOOK_URL=your_slack_webhook_url_here
VITE_BEDROCK_PAGERDUTY_INTEGRATION_KEY=your_pagerduty_key_here
VITE_BEDROCK_WEBHOOK_URL=your_webhook_url_here

# Production Settings (for production environment)
VITE_BEDROCK_CIRCUIT_BREAKER_ENABLED=false
VITE_BEDROCK_CIRCUIT_BREAKER_THRESHOLD=5
VITE_BEDROCK_CIRCUIT_BREAKER_TIMEOUT=60000
VITE_BEDROCK_AUDIT_TRAIL_ENABLED=false
VITE_BEDROCK_GDPR_COMPLIANCE_ENABLED=false
VITE_BEDROCK_PII_DETECTION_ENABLED=false

# Testing Configuration (for staging environment)
VITE_BEDROCK_ENABLE_CANARY_TESTING=false
VITE_BEDROCK_CANARY_PERCENTAGE=10