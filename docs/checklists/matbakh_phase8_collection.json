{
  "info": {
    "name": "Matbakh Phase 8 \u2013 S3 Migration Collection",
    "_postman_id": "c7b1ea8e-8f4a-4fda-a5aa-1234567890ab",
    "description": "Presign, upload, commit, and access URL endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get Presigned URL",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{auth_bearer}}"
          }
        ],
        "url": {
          "raw": "{{api_base}}/get-presigned-url",
          "host": [
            "{{api_base}}"
          ],
          "path": [
            "get-presigned-url"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"bucket\": \"matbakh-files-profile\", \"filename\": \"avatar.jpg\", \"contentType\": \"image/jpeg\", \"folder\": \"avatars/{{user_id}}\", \"fileSize\": 12345}"
        }
      }
    },
    {
      "name": "Complete Multipart Upload",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{auth_bearer}}"
          }
        ],
        "url": {
          "raw": "{{api_base}}/complete-multipart-upload",
          "host": [
            "{{api_base}}"
          ],
          "path": [
            "complete-multipart-upload"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"multipartUploadId\": \"{{multipart_id}}\", \"fileUrl\": \"{{file_url}}\", \"parts\": [{\"partNumber\": 1, \"etag\": \"REPLACE_ETAG\"}]}"
        }
      }
    },
    {
      "name": "Get File Access URL",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{auth_bearer}}"
          }
        ],
        "url": {
          "raw": "{{api_base}}/get-file-access-url",
          "host": [
            "{{api_base}}"
          ],
          "path": [
            "get-file-access-url"
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\"bucket\": \"matbakh-files-profile\", \"key\": \"avatars/{{user_id}}/avatar.jpg\", \"expiresIn\": 3600}"
        }
      }
    }
  ],
  "variable": [
    {
      "key": "api_base",
      "value": "https://api.matbakh.app"
    },
    {
      "key": "auth_bearer",
      "value": "REPLACE_TOKEN"
    },
    {
      "key": "user_id",
      "value": "REPLACE_USER_ID"
    }
  ]
}