{
  "name": "matbakh-get-presigned-url",
  "version": "1.0.0",
  "description": "Lambda function to generate presigned URLs for S3 uploads",
  "main": "dist/index.js",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "@aws-sdk/client-secrets-manager": "^3.879.0",
    "@aws-sdk/s3-request-presigner": "^3.637.0",
    "jose": "^5.9.3",
    "pg": "^8.16.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.129",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.15.5",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "package": "npm run build && zip -r function.zip dist/ node_modules/ -x 'node_modules/typescript/*' 'node_modules/@types/*' 'node_modules/jest/*' 'node_modules/ts-jest/*'",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "lint": "tsc --noEmit"
  }
}
